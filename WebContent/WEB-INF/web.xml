<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">


<!-- Servlet gérant la création d'un client -->

<servlet>
 <servlet-name>CreationClient</servlet-name>
 <servlet-class>com.eStore.servlets.CreationClient</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>CreationClient</servlet-name>
<url-pattern>/creerClient</url-pattern>
</servlet-mapping>

<!-- Servlet gérant la connexion au site -->
<servlet>
 <servlet-name>ConnectionServlet</servlet-name>
 <servlet-class>com.eStore.servlets.ConnectionServlet</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>ConnectionServlet</servlet-name>
<url-pattern>/connect</url-pattern>
</servlet-mapping>

</web-app>